@page "/"
@namespace Xamarin.Forms.Platforms.Blazor.TestApp.ServerSideBlazor.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Xamarin.Forms.Platforms.Blazor.TestApp.ServerSideBlazor</title>
    <base href="~/" />
</head>
<body>
    <app>
        @(await Html.RenderComponentAsync<App>(RenderMode.Server))
    </app>

    <script src="_framework/blazor.server.js"></script>
    <script src="_content/Xamarin.Forms.Platform.Blazor/Xamarin.Forms.Platform.Blazor.js"></script>
</body>
</html>
